{
  "name": "searchable-book-full-stack",
  "version": "1.0.0",
  "description": "MDST (May doc sach tot) Books! full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server/index.js",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "NODE_ENV=dev concurrently \"npm run server\" \"npm run client\"",
    "analyze": "webpack --mode production --json > ./webpack-analyze.log && webpack-bundle-analyzer ./webpack-analyze.log ./dist",
    "dockerbuild": "docker build . -t haoict/searchable-book",
    "dockerpush": "docker push haoict/searchable-book"
  },
  "author": "Hao Nguyen",
  "license": "ISC",
