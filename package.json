    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mime": "^2.3.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.2.12",
    "multer": "^1.3.1",
    "nodemailer": "^4.6.8",
    "prop-types": "^15.6.2",
    "react": "^16.3.1",
    "react-autosuggest": "^9.4.0",
    "react-bootstrap-table": "^4.3.1",
    "react-code-splitting": "^1.2.1",
    "react-countdown-now": "^1.3.0",
    "react-dom": "^16.3.1",
    "react-dropzone": "^5.0.1",
    "react-froala-wysiwyg": "^2.8.4",
    "react-grid-layout": "^0.16.6",
    "react-key-index": "^0.1.1",
    "react-moment": "^0.7.9",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.3.0",
    "redux": "^4.0.0",
